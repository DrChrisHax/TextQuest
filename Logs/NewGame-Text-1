public static void newGame(){
		
		Scanner sc = new Scanner(System.in);
		Race race;
		Type type;
		

		System.out.println("You open your eyes and find yourself in a new world.(Press enter to continue)");
		sc.nextLine();
		System.out.println("What should we call you?");
		String name = sc.nextLine();
		System.out.println("Ah," + name + ", welcome to the world of Textisto.");
		sc.nextLine();
		System.out.println("Now, of what race were you born?");
		System.out.println("(Human, Dwarf, Elf)");
		String raceName = sc.nextLine();
		if(raceName.toLowerCase() == "elf"){
			race = new Elf();
		}
		else if(raceName.toLowerCase() == "dwarf"){
			race = new Dwarf();
		}
		else{
			race = new Human();
		}
		System.out.println("Hmm, " + name + " the " + race.getRace() + ".");
		sc.nextLine();
		System.out.println("In Textiso there are many different classes to chose from. Pick one that best suits your style.");
		System.out.println("(Cleric, Thief, Warrior, Wizard)");
		String typeName = sc.nextLine();
		if(typeName.toLowerCase() == "Cleric"){
			type = new Cleric();
		}
		else if(typeName.toLowerCase() == "Thief"){
			type = new Thief();
		}
		else if(typeName.toLowerCase() == "Wizard") {
			type = new Wizard();
		}
		else{
			type = new Warrior();
		}
		character player = new character(name, race, type);
		
		
		



	}